{
    "name": "YouTube Shortcuts",
    "version": "0.0.1",
    "description": "Additional shortcuts for YouTube",
    "permissions": [],
    "background": {
        "scripts": ["js/background.js"],
        "persistent": false
    },
    "commands": {
        "playback_speed_normal": {
            "suggested_key": {
                "default": "Alt+0"
            },
            "description": "Set to the playback speed 'Normal'"
        },
        "playback_speed_2": {
            "suggested_key": {
                "default": "Alt+1"
            },
            "description": "Set to the playback speed '2'"
        }
    },
    "icons": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
    },
    "manifest_version": 2
}